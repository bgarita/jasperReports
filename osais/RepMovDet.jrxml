	<parameter name="pQuery" class="java.lang.String">
		<defaultValueExpression><![CDATA["SELECT     " +
"a.artcode, " +
"a.bodega,  " +
"c.artdesc, " +
"a.movtimo, " +
"If(a.movtimo = 'E',a.movcant,0) as Entradas,"    +
"If(a.movtimo = 'S',a.movcant,0) as Salidas, "    +
"a.movcoun, " +
"b.tipoca,  " +
"a.movcant * a.movcoun * b.tipoca as CostoTotal," +
"a.CentroC, " +
"a.movdocu, " +
"b.movfech, " +
"b.user,    " +
"(Select empresa from config) as empresa         " +
"FROM hinmovimd a                                " +
"INNER JOIN hinmovime b ON a.movdocu = b.movdocu " +
"INNER JOIN inarticu c ON a.artcode = c.artcode  " +
"AND a.movtimo = b.movtimo                       " +
"AND a.movtido = b.movtido                       " +
"" +
"UNION ALL  " +
"" +
"SELECT     " +
"a.artcode, " +
"a.bodega,  " +
"c.artdesc, " +
"a.movtimo, " +
"If(a.movtimo = 'E',a.movcant,0) as Entradas,"    +
"If(a.movtimo = 'S',a.movcant,0) as Salidas, "    +
"a.movcoun, " +
"b.tipoca,  " +
"a.movcant * a.movcoun * b.tipoca as CostoTotal," +
"a.CentroC, " +
"a.movdocu, " +
"b.movfech, " +
"b.user,    " +
"(Select empresa from config) as empresa        " +
"FROM inmovimd a                                " +
"INNER JOIN inmovime b ON a.movdocu = b.movdocu " +
"INNER JOIN inarticu c ON a.artcode = c.artcode " +
"AND a.movtimo = b.movtimo                      " +
"AND a.movtido = b.movtido "]]></defaultValueExpression>
	</parameter>
